          "PAUSE",
          "STOP",
          "AMS_BACK",
          "AMS_FORWARD",
          "AMS_REWIND",
          "AMS_FASTFORWARD",
          "DISC_SKIP_BACK",
          "DISC_SKIP_FORWARD"
        ]
      },
      "IrCodeDto": {
        "type": "object",
        "properties": {
          "code": {
            "allOf": [
              {
                "$ref": "#/components/schemas/IrCode"
              }
            ]
          }
        },
        "required": [
          "code"
        ]
      },
      "Disc": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "position": {
            "type": "integer",
            "nullable": true
          },
          "dateCreated": {
            "format": "date-time",
            "type": "string"
          },
          "artist": {
            "type": "string"
          },
          "album": {
            "type": "string"
          },
          "tracks": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Track"
            }
          }
        },
        "required": [
          "uuid",
          "position",
          "dateCreated",
          "artist",
          "album",
          "tracks"
        ]
      },
      "Track": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "index": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "disc": {
            "$ref": "#/components/schemas/Disc"
          }
        },
        "required": [
          "uuid",
          "index",
          "title"
        ]
      },
      "CreateDiscDto": {
        "type": "object",
        "properties": {
          "artist": {
            "type": "string"
          },
          "album": {
            "type": "string"
          },
          "position": {
            "type": "integer"
          }
        },
        "required": [
          "artist",
          "album"
        ]
      },
      "UpdateDiscDto": {
        "type": "object",
        "properties": {
          "artist": {
            "type": "string"
          },
          "album": {
            "type": "string"
          },
          "position": {
            "type": "integer"
          }
        }
      },
      "PlayDiscDto": {
        "type": "object",
        "properties": {
          "trackUuid": {
            "type": "string"
          }
        }
      },
      "AddTrackDto": {
        "type": "object",
        "properties": {
