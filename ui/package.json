{
	"name": "jukeblade-ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "npm run set-api-url && vite",
		"build": "npm run set-api-url && tsc -b && vite build",
		"lint": "npm run set-api-url && eslint .",
		"preview": "vite preview",
		"format": "prettier --write \"src/**/*.ts\"",
		"gen:types": "openapi-typescript ../oas3-api.json -o src/api-schema.ts --enum --root-types --root-types-no-schema-prefix",
		"set-api-url": "node scripts/set-api-url"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^7.3.2",
		"@mui/material": "^7.3.2",
		"jukeblade-typescript-client": "file:../api/client/typescript",
		"openapi-fetch": "^0.14.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.9.1",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^5.0.0",
		"dotenv": "^17.2.2",
		"eslint": "^9.33.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"openapi-typescript": "^7.9.1",
		"prettier": "^3.6.2",
		"sass": "^1.92.1",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2"
	},
	"volta": {
		"node": "22.19.0"
	}
}
